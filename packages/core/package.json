{
  "name": "@whiteout/core",
  "version": "0.1.0",
  "type": "module",
  "files": [
    "dist",
    "data"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "whiteout": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./anonymize": {
      "import": "./dist/anonymize.js",
      "types": "./dist/anonymize.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "bundle:iife": "vite build --config vite.bundle.config.ts",
    "bundle": "tsc && vite build --config vite.bundle.config.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@bufbuild/protobuf": "^2.0",
    "@connectrpc/connect": "^2.0",
    "@connectrpc/connect-web": "^2.0",
    "@types/node": "^25.3.0",
    "typescript": "^5.7",
    "vite": "^6.0",
    "vitest": "^3.0"
  }
}
